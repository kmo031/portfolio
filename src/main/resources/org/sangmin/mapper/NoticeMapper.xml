<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.sangmin.mapper.NoticeMapper">

	<select id="getList" resultType="org.sangmin.domain.NoticeDTO">
		SELECT * FROM notice
	</select>
	<select id="read" resultType="org.sangmin.domain.NoticeDTO">
		SELECT * FROM notice WHERE id = #{id}
	</select>


</mapper>